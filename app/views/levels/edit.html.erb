
<h1> Edit Blood Gluclose Reading</h1>

  <%= form_tag '/levels', method: :patch do %>

    <div>
    <%= label_tag :user_id %>
    <%= text_field_tag :user_id%>
    </div>
    
    <div>
    <%= label_tag :reading %>
    <%= text_field_tag :reading%>
    </div>

    <div>
    <%= label_tag :date %>
    <%= text_field_tag :date%>
    </div>

    <div>
    <%= label_tag :time %>
    <%= text_field_tag :time%>
    </div>

    <div>
    <%= submit_tag 'Update' %>
    </div>

  <% end %>

