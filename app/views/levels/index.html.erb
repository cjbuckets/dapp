<% @levels.each do |level| %>
  <h4> <a href="/levels/<%= level.id %>"><%= level.reading %></a></h4>
  <h5>Date: <%= level.date %></h5>
  <h5>Time: <%= level.time %></h5>
<br>
<% end %>

<p><%= link_to "Printable Levels Summary", levels_path(@levels, format: "pdf") %></p>


<h5><%= link_to "Activities", "/activities", class: "btn btn-success" %></h5> 
